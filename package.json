{
  "name": "nas-proxy-entry",
  "version": "0.1.0",
  "description": "Entry page for personal NAS proxy with on-demand whitelisting.",
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "node-config-ts && env-prompt",
    "build": "rimraf dist && tsc",
    "build-watch": "rimraf dist && tsc -w",
    "lint": "tslint 'src/**/*.ts' 'test/**/*.test.ts'",
    "start": "node dist/index.js",
    "start-production": "cross-env NODE_ENV=production node dist/index.js",
    "start-development": "cross-env NODE_ENV=development node dist/index.js"
  },
  "author": "Jozef Zsapka",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.2",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "env-prompt": "^1.0.7",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "node-config-ts": "^3.0.4",
    "pino": "^6.0.0",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@types/async": "^3.0.1",
    "@types/express": "^4.11.1",
    "@types/node": "^12.7.5",
    "@types/http-errors": "^1.6.1",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "tsc-watch": "^3.0.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.6.3"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
